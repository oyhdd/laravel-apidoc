{
    "name": "wealedger/apidoc",
    "description": "自动生成在线测试接口和文档",
    "keywords": ["package", "library"],
    "license": "MIT",
    "type": "library",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.ngochain.cn"
        },
        {
            "packagist": false
        }
    ],
    "require": {
    },
    "autoload": {
        "psr-4": {
            "Wealedger\\Document\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "secure-http" : false,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wealedger\\Document\\DocumentServiceProvider"
            ]
        }
    }
}

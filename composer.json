{
    "name": "oyhdd/laravel-apidoc",
    "description": "自动生成在线测试接口和文档",
    "keywords": ["package", "library"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "wangmeng",
            "email": "1040089624@qq.com"
        }
    ],
    "require": {
    },
    "autoload": {
        "psr-4": {
            "Oyhdd\\Document\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "secure-http" : false,
